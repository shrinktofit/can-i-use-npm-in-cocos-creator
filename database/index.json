            }
        },
        "whatwg-fetch": {
            "usage": {
                "module": "commonjs",
                "path": ".",
                "local": "fetch"
            }
        },
        "xstate": {
            "types": {
                "definitelyTyped": false,
                "subPathTypes": false
            },
            "usage": {
