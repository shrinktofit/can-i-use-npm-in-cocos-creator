{
    "$schema": "./schema.json",
    "packages": {
        "astar-typescript": {
            "usage": {
                "module": "commonjs",
                "path": ".",
                "local": "astar"
            }
        },
        "async": {
            "types": {
                "definitelyTyped": true,
                "subPathTypes": false
            },
            "usage": {
                "module": "module",
                "path": "dist/async.mjs",
                "export": {
                    "type": "named",
                    "exports": ["forEachOf"]
                }
            }
        },
        "async-es": {
            "usage": {
                "module": "module",
                "path": ".",
                "export": {
                    "type": "named",
                    "exports": ["forEachOf"]
                }
            }
        },
        "axios": {
            "types": {
                "definitelyTyped": false,
                "subPathTypes": false
            },
            "usage": {
                "module": "commonjs",
                "path": "dist/axios.min.js",
                "local": "axios"
            }
        },
        "crypto-ts": {
            "usage": {
                "module": "commonjs",
                "path": ".",
                "local": "crypto"
            }
        },
        "fairygui-cc": {
            "usage": {
                "module": "module",
                "path": ".",
                "export": {
                    "type": "namespace",
                    "local": "fgui"
                }
            }
        },
        "jszip": {
            "types": {
                "definitelyTyped": true,
                "subPathTypes": false
            },
            "usage": {
                "module": "commonjs",
                "path": "dist/jszip.min.js",
                "local": "jszip"
            }
        },
        "lodash": {
            "usage": {
                "module": "commonjs",
                "path": "array.js",
                "local": "array"
            }
        },
        "lodash-es": {
            "usage": {
                "module": "module",
                "path": "zip.js",
                "export": {
                    "type": "default",
                    "local": "zip"
                }
            }
        },
        "mobx": {
            "types": {
                "definitelyTyped": false,
                "subPathTypes": true
            },
            "usage": [{
                "module": "module",
                "path": "dist/mobx.esm.js",
                "export": [
                    { "type": "named", "exports": ["ObservableMap", "observable"] }
                ]
            }, {
                "module": "commonjs",
                "path": "dist/mobx.cjs.development.js",
                "local": "mobx"
            }]
        },
        "msgpackr": {
            "types": {
                "definitelyTyped": false,
                "subPathTypes": true
            },
            "usage": {
                "module": "module",
                "path": ".",
                "export": [
                    { "type": "named", "exports": ["pack", "unpack"] }
                ]
            }
        },
        "pako": {
            "types": {
                "definitelyTyped": true,
                "subPathTypes": false
            },
            "usage": {
                "module": "module",
                "path": ".",
                "export": [
                    { "type": "default", "local": "pako" },
                    { "type": "named", "exports": ["Deflate", "Inflate"] }
                ]
            }
        },
        "rxjs": {
            "usage": {
                "module": "commonjs",
                "path": "dist/bundles/rxjs.umd.js",
                "local": "rxjs"
            }
        },
        "web3": {
            "types": {
                "definitelyTyped": true,
                "subPathTypes": false
            },
            "usage": {
                "module": "commonjs",
                "path": "dist/web3.min.js",
